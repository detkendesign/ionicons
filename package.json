{
  "name": "@dtkn/ionicons",
  "version": "0.0.2",
  "description": "An icon component for ionicons",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "create-icons": "tsx scripts/create-icon.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:dev": "vitest",
    "ci": "pnpm run build && pnpm run format:check && pnpm run check-exports && pnpm run test",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "pnpm run ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detkendesign/ionicons.git"
  },
  "keywords": [
    "icons",
    "ionicons",
    "ionicons-react",
    "react-ionicons",
    "react"
  ],
  "author": "Anton Detken <anton@detken.dev> (https://detkendevelopment.com)",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/detkendesign/ionicons/issues"
  },
  "homepage": "https://github.com/detkendesign/ionicons#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.8.4",
    "@types/react": "^18.3.12",
    "ionicons": "^7.4.0",
    "prettier": "^3.3.3",
    "svg-to-jsx": "^1.0.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "react": "^18.3.1"
  }
}
