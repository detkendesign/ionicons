{
  "name": "@dtkn/ionicons",
  "version": "0.0.2",
  "description": "An icon component for ionicons",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:dev": "vitest",
    "ci": "pnpm build && pnpm format:check && pnpm check-exports && pn test",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "pnpm ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detkendesign/ionicons.git"
  },
  "keywords": [
    "icons",
    "ionicons",
    "ionicons-react",
    "react-ionicons",
    "react"
  ],
  "author": "Anton Detken <anton@detken.dev> (https://detkendevelopment.com)",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/detkendesign/ionicons/issues"
  },
  "homepage": "https://github.com/detkendesign/ionicons#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
